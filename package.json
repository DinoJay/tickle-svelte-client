    "firebase": "^9.8.2",
    "firebase-functions": "^3.21.2",
    "immer": "^9.0.14",
    "mapbox-gl": "^2.8.2",
    "qs": "^6.10.5",
    "svelte-material-icons": "^2.0.2",
    "svelte-spa-router": "^3.2.0",
    "uid": "^2.0.0",
    "uuidv4": "^6.2.13"
  }
